#include <iostream>
#include<time.h>

int amin(int tab[], int n)
{
    int licznik=tab[0];
    for(int i=0;i<n;i++)
    {
       if(tab[i]<licznik)
       {
           licznik=tab[i];
       }
    }
    return licznik;
}
int main()
{
    int n;
    int tab[]={12, -8 , 17, 22, 4, -2, -7, 8, -11};
    std::cout<<"Podaj rozmiar tablicy: \n";
    std::cin>>n;
    std::cout<<amin(tab, n);
    return 0;
}
