#include <iostream>
#include<time.h>

int asum(int tab[], int n)
{
    int licznik=0;
    for(int i=0;i<n;i++)
    {
        srand(time(NULL));
        tab[i] = rand() % 100 + 1;
        licznik=tab[i]+licznik;
    }
    return licznik;
}
int main()
{
    int n;
    int tab[10];
    std::cout<<"Podaj rozmiar tablicy: \n";
    std::cin>>n;
    std::cout<<asum(tab, n);
    return 0;
}
