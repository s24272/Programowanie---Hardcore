#include <iostream>

int main()
{
    int a;
    std::cout << "Dlugosc boku: ";
    std::cin>>a;

    for(int i=1;i<=a;++i)
    {
        std::cout<<"*";
    }
    std::cout<<"\n";
    for(int i=0;i<a-2;i++)
    {
        std::cout<<"*";
        for(int j=1;j<=a-2;++j)
        {
            std::cout<<" ";
        }
        std::cout<<"*\n";
    }

    for(int k=1;k<=a;++k)
    {
        std::cout<<"*";
    }

}
