#include <iostream>
#include<time.h>

int search(int tab[], int n, int needle)
{
    int licznik=0;
    for(int i=0;i<n;i++)
    {
        srand(time(NULL));
        tab[i] = rand() % 100 + 1;
        licznik=tab[i]+licznik;
       if(tab[i]==needle)
       {
           return i;
       }
    }
    return -1;
}
int main()
{
    int n, needle;
    int tab[100];
    std::cout<<"Podaj rozmiar tablicy: \n";
    std::cin>>n;
    std::cout<<"Podaj wartosc do znalezienia: \n";
    std::cin>>needle;
    std::cout<<search(tab, n, needle);
    return 0;
}
