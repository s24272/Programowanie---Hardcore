#include <iostream>
#include <cstdlib>
#include <time.h>

void sort_desc(int tab[], int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(tab[j]<tab[j+1])
            {
                   std::swap(tab[j], tab[j+1]);
            }
        }
    }
}

int main()
{
    int n;
    int tab[]={7, 12, 3, 19, 26, 24, 2, 33, 8, 24};
    std::cout<<"Podaj dlugosc tablicy: \n";
    std::cin>>n;
    sort_desc(tab, n);
    for(int i=0;i<n;i++)
    {
        std::cout<<tab[i]<<"\n";
    }
    return 0;
}
