#include <iostream>

void iota(int tab[],int n, int start)
{
    for(int i=0;i<n;i++)
    {
        tab[i]=start;
        start++;
        std::cout<<tab[i]<<"\n";
    }
}
int main()
{
    int n, start;
    int tab[1000];
    std::cout<<"Podaj rozmiar tablicy: \n";
    std::cin>>n;
    std::cout<<"Podaj od jakiej liczby mamy zapisywac \n";
    std::cin>>start;
    iota(tab, n, start);
    return 0;
}
