#include <iostream>

int is_divisible(int n, int l)
{
	if(n%l==0)
	{
		return 1;
	}
	return 0;
}
bool is_even(int n, int l)
{
	if(is_divisible(n,l))
	{
		return true;
	}
	return false;
}
int main()
{
	int n,l;
	std::cout<<"Podaj dwie liczby: ";
	std::cin>>n>>l;
	if(is_even(n,l))
	{
		std::cout<<"Jest podzielna";
	}
	else
	{
		std::cout<<"Jest niepodzielna";
	}
	return 0;
}
