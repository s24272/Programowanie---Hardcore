#include <iostream>

int main()
{
    std::string x[5];
    x[0]=" Hello ";
    x[1]=" World ";
    x[2]=" in    ";
    x[3]=" a     ";
    x[4]=" frame ";
    for(int i=1; i<=9; i++)
    {
        std::cout << "*";
    }
    std::cout<<std::endl;
   
 	for(int i=0; i<5; i++)
	{
    	std::cout<<"*";
        std::cout<< x[i];
    	
    	std::cout<<"*"<<std::endl;
	}
   
    for(int k=1; k<=9; k++)
    {
        std::cout<<"*";
    }
   
}
