#include <iostream>


int main()
{
    int a,b,s;
    std::cout << "Podaj dwie liczby:";
    std::cin>>a>>b>>s;
    if(a<b)
    {
        for(a;a<=b;a=a+s)
        {
            std::cout<<a<<"\n";
        }
    }
    else if (a>b)
    {
        for(b;b<=a;b=b+s)
        {
            std::cout<<b<<"\n";
        }
    }
    else
    {
        std::cout<<"liczby sa takie same";
    }


    return 0;
}
