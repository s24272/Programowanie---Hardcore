#include<iostream>

char znak(char* a, char* b)
{
    if(a>b)
    {
        return '>';
    }
    else if(a<b)
    {
        return '<';
    }
    else if(a==b)
    {
        return '=';
    }
}

int main(int argc, char* argv[])
{
    argc=2;
    std::cout<<argv[1]<<znak(argv[1],argv[2])<<argv[2]<<"\n";
    return 0;
}

