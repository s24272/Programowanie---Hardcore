#include <iostream>

char znak(int a, int b)
{
    if(a>b)
    {
        return '>';
    }
    else if(a<b)
    {
        return '<';
    }
    else
    {
        return '=';
    }
}

int main()
{
    int a, b;
    std::cout << "Podaj dwie liczby:\n";
    std::cin>>a;
    for(int i=0;i<5;i++)
    {
        std::cin>>b;
        std::cout<<a<<znak(a,b)<<b<<"\n";
    }


    return 0;
