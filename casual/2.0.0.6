
#include<iostream>

char znak(char* a, char* b)
{
    if(a>b)
    {
        return '>';
    }
    else if(a<b)
    {
        return '<';
    }
    else if(a==b)
    {
        return '=';
    }
}

int main(int argc, char* argv[])
{
    argc=6
     for(int i=0;i<6;i++)
    {
       std::cout<<argv[i]<<znak(argv[i],argv[i+1])<<argv[i+1]<<"\n";
    }
    
    return 0;
}
