#include <iostream>


int main()
{
    int a,b,c;
    std::cout << "Podaj trzy liczby:";
    std::cin>>a>>b>>c;
    if(a<b)
    {
        for(a;a<=b;a++)
        {
            if(a%c==0)
                std::cout<<a<<"\n";
        }
    }
    else if (a>b)
    {
        for(b;b<=a;b++)
        {
            if(b%c==0)
                std::cout<<b<<"\n";
        }
    }
    else
    {
        std::cout<<"liczby sa takie same";
    }


    return 0;
}
